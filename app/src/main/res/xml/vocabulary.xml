<?xml version="1.0" encoding="utf-8"?>
<Questions xmlns="https://www.noprestige.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="https://www.noprestige.com questions.xsd">
    <KanaSet prefId="vocabulary_set_basic"
             setTitle="@string/vocab_set_basic_title">
        <WordQuestion romanji="Hai" kana="はい" answer="Yes"/>
        <WordQuestion romanji="Iie" kana="いいえ" answer="No"/> <!-- 否 (not normally used) -->
        <WordQuestion romanji="Watashi" kana="わたし" kanji="私" answer="I"/>
        <WordQuestion romanji="Anata" kana="あなた" kanji="貴方" answer="You"/>
        <!-- Re: Anata: The kanji can be gendered: 貴女 refers only to females and 貴男 refers only to males -->
    </KanaSet>
    <KanaSet prefId="vocabulary_set_study"
             setTitle="@string/vocab_set_study_title">
        <WordQuestion romanji="Pen" kana="ペン" answer="Pen"/> <!--Katakana-->
        <WordQuestion romanji="Enpitsu" kana="えんぴつ" kanji="鉛筆" answer="Pencil"/>
        <WordQuestion romanji="Tsukue" kana="つくえ" kanji="机" answer="Desk"/>
        <WordQuestion romanji="Isu" kana="いす" kanji="椅子" answer="Chair"/>
        <WordQuestion romanji="Hon" kana="ほん" kanji="本" answer="Book"/>
        <WordQuestion romanji="Noto" kana="ノート" answer="Notebook"/> <!--Katakana-->
    </KanaSet>
    <KanaSet prefId="vocabulary_set_animals"
             setTitle="@string/vocab_set_animals_title">
        <WordQuestion romanji="Inu" kana="いぬ" kanji="犬" answer="Dog"/>
        <WordQuestion romanji="Neko" kana="ねこ" kanji="猫" answer="Cat"/>
        <WordQuestion romanji="Uma" kana="うま" kanji="馬" answer="Horse"/>
        <WordQuestion romanji="Buta" kana="ぶた" kanji="豚" answer="Pig"/>
        <WordQuestion romanji="Ika" kana="いか" kanji="烏賊" answer="Squid"/>
        <WordQuestion romanji="Ahiru" kana="あひる" kanji="家鴨" answer="Duck"/>
        <WordQuestion romanji="Ushi" kana="うし" kanji="牛" answer="Cow"/>
        <WordQuestion romanji="Sakana" kana="さかな" kanji="魚" answer="Fish"/>
        <WordQuestion romanji="Tori" kana="とり" kanji="鳥" answer="Bird"/>
        <WordQuestion romanji="Kame" kana="かめ" kanji="亀" answer="Turtle"/>
    </KanaSet>
    <KanaSet prefId="vocabulary_set_transport"
             setTitle="@string/vocab_set_transport_title">
        <WordQuestion romanji="Densha" kana="でんしゃ" kanji="電車" answer="Train"/>
        <WordQuestion romanji="Jitensha" kana="じてんしゃ" kanji="自転車" answer="Bicycle"/>
        <WordQuestion romanji="Jidousha" kana="じどうしゃ" kanji="自動車" answer="Car"/> <!-- Jidōsha -->
        <WordQuestion romanji="Hikouki" kana="ひこうき" kanji="飛行機" answer="Airplane"/> <!-- Hikōki -->
        <WordQuestion romanji="Basu" kana="バス" answer="Bus"/> <!--Katakana-->
        <WordQuestion romanji="Fune" kana="ふね" kanji="船" answer="Boat"/>
        <WordQuestion romanji="Eki" kana="えき" kanji="駅" answer="Rail Station"/>
        <WordQuestion romanji="Kuukou" kana="くうこう" kanji="空港" answer="Airport"/> <!-- Kūkō -->
        <WordQuestion romanji="Shinkansen" kana="しんかんせん" kanji="新幹線" answer="Bullet Train"/>
        <WordQuestion romanji="Kippu" kana="きっぷ" kanji="切符" answer="Ticket"/>
    </KanaSet>
    <KanaSet prefId="vocabulary_set_people"
             setTitle="@string/vocab_set_people_title">
        <!--<WordQuestion romanji="San" kana="さん" answer="Honorific"/>-->
        <WordQuestion romanji="Sensei" kana="せんせい" kanji="先生" answer="Teacher"/>
        <WordQuestion romanji="Tomodachi" kana="ともだち" kanji="友達" answer="Friend"/>
        <WordQuestion romanji="Kazoku" kana="かぞく" kanji="家族" answer="Family"/>
        <WordQuestion romanji="Otousan" kana="おとうさん" kanji="お父さん" answer="Father"/> <!-- Otōsan -->
        <WordQuestion romanji="Okaasan" kana="おかあさん" kanji="お母さん" answer="Mother"/>
        <WordQuestion romanji="Oniisan" kana="おにいさん" kanji="お兄さん" answer="Elder Brother"/>
        <WordQuestion romanji="Otouto" kana="おとうと" kanji="弟" answer="Younger Brother"/> <!-- Otōto -->
        <WordQuestion romanji="Oneesan" kana="おねえさん" kanji="お姉さん" answer="Elder Sister"/>
        <WordQuestion romanji="Imouto" kana="いもうと" kanji="妹" answer="Younger Sister"/> <!-- Imōto -->
    </KanaSet>
    <KanaSet prefId="vocabulary_set_food"
             setTitle="@string/vocab_set_food_title">
        <WordQuestion romanji="Kudamono" kana="くだもの" kanji="果物" answer="Fruit"/>
        <WordQuestion romanji="Yasai" kana="やさい" kanji="野菜" answer="Vegetable"/>
        <WordQuestion romanji="Mizu" kana="みず" kanji="水" answer="Water"/>
        <WordQuestion romanji="Ocha" kana="おちゃ" kanji="お茶" answer="Green Tea"/>
        <WordQuestion romanji="Gyuunyuu" kana="ぎゅうにゅう" kanji="牛乳" answer="Milk"/> <!-- Gyūnyū -->
        <WordQuestion romanji="Niku" kana="にく" kanji="肉" answer="Meat"/>
        <WordQuestion romanji="Pan" kana="パン" answer="Bread"/> <!--Katakana-->
        <WordQuestion romanji="Asagohan" kana="あさごはん" kanji="朝ご飯" answer="Breakfast"/>
        <WordQuestion romanji="Hirugohan" kana="ひるごはん" kanji="昼ご飯" answer="Lunch"/>
        <WordQuestion romanji="Bangohan" kana="ばんごはん" kanji="晩御飯" answer="Dinner"/>
    </KanaSet>
    <KanaSet prefId="vocabulary_set_home"
             setTitle="@string/vocab_set_home_title">
        <WordQuestion romanji="Uchi" kana="うち" kanji="家" answer="House"/>
        <WordQuestion romanji="Heya" kana="へや" kanji="部屋" answer="Room"/>
        <WordQuestion romanji="Beddo" kana="ベッド" answer="Bed"/> <!--Katakana-->
        <WordQuestion romanji="Makura" kana="まくら" kanji="枕" answer="Pillow"/>
        <WordQuestion romanji="Moufu" kana="もうふ" kanji="毛布" answer="Blanket"/> <!-- Mōfu-->
        <WordQuestion romanji="Ofuro" kana="おふろ" kanji="お風呂" answer="Bath"/>
        <WordQuestion romanji="Mado" kana="まど" kanji="窓" answer="Window"/>
        <WordQuestion romanji="Taoru" kana="タオル" answer="Towel"/> <!--Katakana-->
        <WordQuestion romanji="Kagami" kana="かがみ" kanji="鏡" answer="Mirror"/>
        <WordQuestion romanji="Denwa" kana="でんわ" kanji="電話" answer="Phone"/>
    </KanaSet>
    <KanaSet prefId="vocabulary_set_numbers"
             setTitle="@string/vocab_set_numbers_title">
        <WordQuestion romanji="Rei" kana="れい" kanji="零" answer="0"/> <!-- 〇 (maru) is valid -->
        <!--<WordQuestion romanji="Zero" kana="ゼロ" kanji="零" answer="0"/>-->
        <WordQuestion romanji="Ichi" kana="いち" kanji="一" answer="1"/>
        <WordQuestion romanji="Ni" kana="に" kanji="二" answer="2"/>
        <WordQuestion romanji="San" kana="さん" kanji="三" answer="3"/>
        <WordQuestion romanji="Yon" kana="よん" kanji="四" answer="4"/>
        <!--<WordQuestion romanji="Shi" kana="し" kanji="四" answer="4"/>-->
        <WordQuestion romanji="Go" kana="ご" kanji="五" answer="5"/>
        <WordQuestion romanji="Roku" kana="ろく" kanji="六" answer="6"/>
        <WordQuestion romanji="Nana" kana="なな" kanji="七" answer="7"/>
        <!--<WordQuestion romanji="Shichi" kana="しち" kanji="七" answer="7"/>-->
        <WordQuestion romanji="Hachi" kana="はち" kanji="八" answer="8"/>
        <WordQuestion romanji="Kyuu" kana="きゅう" kanji="九" answer="9"/> <!-- Kyū -->
        <WordQuestion romanji="Juu" kana="じゅう" kanji="十" answer="10"/> <!-- Jū -->
    </KanaSet>
    <KanaSet prefId="vocabulary_set_big_numbers"
             setTitle="@string/vocab_set_big_numbers_title">
        <WordQuestion romanji="Hyaku" kana="ひゃく" kanji="百" answer="Hundred"/><!-- 100 or 10^2 -->
        <WordQuestion romanji="Sen" kana="せん" kanji="千" answer="Thousand"/><!-- 1,000 or 10^3 -->
        <WordQuestion romanji="Man" kana="まん" kanji="万" answer="Ten Thousand"/><!-- 10,000 or 10^4 -->
        <WordQuestion romanji="Oku" kana="おく" kanji="億" answer="Hundred Million"/><!-- 100,000,000 or 10^8 -->
        <WordQuestion romanji="Chou" kana="ちょう" kanji="兆" answer="Trillion"/> <!-- Chō -->
        <!-- 1,000,000,000,000 or 10^12 -->
        <WordQuestion romanji="Kei" kana="けい" kanji="京" answer="Ten Quadrillion"/>
        <!-- 10,000,000,000,000,000 or 10^16 -->
    </KanaSet>
    <!--
    Sources:
    Learn Japanese to Survive: Hiragana Battle
    Learn Japanese to Survive: Katakana War
    Jisho (looks up Kanji from kana): https://jisho.org
    Wikipedia: https://en.wikipedia.org/wiki/Japanese_numerals
    -->
</Questions>